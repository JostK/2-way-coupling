FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (1.6497e-310 0 0);

boundaryField
{
    inlet
    {
        type            waveVelocity;
        refValue        uniform (1.64959e-310 0 0);
        refGradient     uniform (0 0 0);
        valueFraction   uniform 1;
        value           uniform (1.64959e-310 0 0);
    }
    bottom
    {
        type            slip;
    }
    outlet
    {
        type            waveVelocity;
        refValue        uniform (0 0 0);
        refGradient     uniform (0 0 0);
        valueFraction   uniform 1;
        value           uniform (0 0 0);
    }
    atmosphere
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }
    frontBack
    {
        type            empty;
    }
}


// ************************************************************************* //
