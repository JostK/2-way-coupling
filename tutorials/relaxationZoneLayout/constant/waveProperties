gravity         gravity [0 1 -2 0 0 0 0] (0 0 -9.81);

seaLevel	0.0;

relaxationNames      (rectangular0 rectangular1 rectangular2 rectangular3 cylindrical semiCylindrical);

rectangular0Coeffs
{
    relaxationZone
    {
        relaxationShape Rectangular;

        relaxType   INLET;
        startX      (0 0 0);
        endX        (10 10 0);
        orientation (1 0 0);
    }
}

