{
    relaxationZone
    {
        relaxationShape SemiCylindrical;
    
        centre   (30 30 0);
        rInner    5;
        rOuter   25;
        zeroAngleDirection (1 0 0);
        angleStart 45;
        angleEnd   220;
    }
}

