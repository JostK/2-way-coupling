FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      loggingProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

surfaceElevation
{
    type waveGauge;

    pointDistribution userDefinedDistribution;
    N 12;
    xValues nonuniform List<scalar> 12(0.1 2.0 4.0 5.2 10.5 12.5 13.5 14.5 15.7 17.3 19.0 21.0);
    yValues uniform -0.09;
    zValues uniform 0.005;
    add       ( 0 0.8 0);
    axis      y;

    // REQURIED ONLY FOR NEW FUNCTIONOBJECT IMPLEMENTATION
    writeControl adjustableRunTime;
    writeInterval 0.025;
}


